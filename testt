import math
s=input("enter numbers seperated by commas")
s=list(map(int,s.split(',')))
print(s)
# 2nd question
print("even numbers are")
[print(item) for item in s if (item%2==0 and item>0)]
#3rd question
maxi=max(s)
print("maximum is",maxi)
mini=min(s)
print("manimum is",mini)
l=['',]
l=[item for item in s if(item not in l)]
for item in s:
    if (item not in l):
        l=l.append(item)
print(l)
